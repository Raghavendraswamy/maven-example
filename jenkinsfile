pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                git url: 'https://github.com/your-repo-url.git', branch: 'main' // Change to your repo URL and branch
            }
        }
        stage('Build') {
            steps {
                sh './build.sh' // Modify based on your build script
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test' // Modify based on your test commands
            }
        }
    }
    post {
        always {
            archiveArtifacts artifacts: '**/target/*.jar', fingerprint: true // Change if needed
        }
    }
}
